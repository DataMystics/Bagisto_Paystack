{
    "name": "razor/paystack",
    "description": "Paystack payment gateway for Bagisto",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "bagisto/bagisto": "^2.2.1"
    },
    "autoload": {
        "psr-4": {
            "Razor\\Paystack\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Razor\\Paystack\\src\\Providers\\PaystackServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DataMystics/Bagisto_Paystack.git"
        }
    ]
}
